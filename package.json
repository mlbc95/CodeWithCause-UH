{
  "name": "codewithcause",
  "version": "1.0.0",
  "description": "Code with a Cause Node Backend",
  "main": "app.js",
  "engines": {
    "node": "8.9.3",
    "npm": "5.5.1"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "routes-gen": "tsoa routes -c custom-tsoa.json",
    "swagger-gen": "tsoa swagger -c custom-tsoa.json",
    "tsoa-gen": "npm run routes-gen && npm run swagger-gen",
    "swagger-copy": "cp ./server-src/swagger.json ./server-build/swagger.json",
    "clean": "rimraf ./server-build && rimraf ./server-src/routes.ts && rimraf ./server-src/swagger.json",
    "start-gen": "npm run clean && npm run swagger-gen && tsc && npm run swagger-copy",
    "heroku-prebuild": "export NPM_CONFIG_PRODUCTION=false; export NODE_ENV=development; npm install --only=dev --dev",
    "heroku-postbuild": "export NPM_CONFIG_PRODUCTION=true; export NODE_ENV=production;",
    "start-dev": "concurrently --kill-others-on-fail \"tsc -w\" \"nodemon ./server-build/index.js\"",
    "start": "tsc && cp ./server-src/swagger.json ./server-build/swagger.json && node ./server-build/index.js"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "config": "^1.29.2",
    "cors": "^2.8.4",
    "express": "^4.16.2",
    "express-winston": "^2.4.0",
    "js-yaml": "^3.10.0",
    "jsonwebtoken": "^8.1.1",
    "moment": "^2.20.1",
    "mongoose": "^5.0.2",
    "morgan": "^1.9.0",
    "newrelic": "^2.6.1",
    "passport": "^0.4.0",
    "passport-jwt": "^3.0.1",
    "swagger-ui-express": "^2.0.15",
    "tsoa": "^2.0.2",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.1",
    "@types/express": "^4.11.0",
    "@types/jsonwebtoken": "^7.2.5",
    "@types/mongoose": "^5.0.0",
    "@types/node": "^9.4.0",
    "@types/passport": "^0.4.2",
    "@types/passport-jwt": "^3.0.0",
    "@types/winston": "^2.3.7",
    "concurrently": "^3.5.1",
    "nodemon": "^1.14.11",
    "rimraf": "^2.6.2",
    "tslint": "^5.9.1",
    "typescript": "^2.6.2"
  }
}
